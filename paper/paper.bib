  urldate = {2024-03-05},
  journal = {Environmental Research},
  author = {Nieuwenhuijsen, Mark and De Nazelle, Audrey and Pradas, Marta Cirach and Daher, Carolyn and Dzhambov, Angel M. and Echave, Cynthia and Gössling, Stefan and Iungman, Tamara and Khreis, Haneen and Kirby, Nicolina and Khomenko, Sasha and Leth, Ulrich and Lorenz, Florian and Matkovic, Vlatka and Müller, Johannes and Palència, Laia and Pereira Barboza, Evelise and Pérez, Katherine and Tatah, Lambed and Tiran, Jernej and Tonne, Cathryn and Mueller, Natalie},
